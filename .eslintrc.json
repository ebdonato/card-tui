{
    "env": {
        "node": true,
        "es2022": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "no-console": "off",
        "prettier/prettier": "error"
    }
}
